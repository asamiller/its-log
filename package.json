  "bugs": {
    "url": "https://github.com/asamiller/its-log/issues"
  },
  "homepage": "https://github.com/asamiller/its-log",
  "dependencies": {
    "lodash": "^2.4.1",
    "string": "^3.0.0",
    "winston": "^0.8.3",
    "winston-orchestrate": "git://github.com/asamiller/winston-orchestrate.git"
  },
  "devDependencies": {
    "chai": "^1.10.0",
    "coveralls": "^2.11.1",
    "dotenv": "^0.4.0",
    "jscoverage": "^0.5.6",
    "jshint": "^2.5.5",
    "mocha": "^1.21.4",
    "mocha-lcov-reporter": "0.0.1",
    "nock": "^0.46.0"
