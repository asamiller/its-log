    "report": "./node_modules/.bin/mocha -R html-cov > coverage.html",
    "coveralls": "./node_modules/.bin/mocha -R mocha-lcov-reporter | ./node_modules/coveralls/bin/coveralls.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/asamiller/its-log.git"
  },
